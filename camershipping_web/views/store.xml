            <body>

            <!-- Page Header Start -->
               <div class="main-images">
                   <div class="container py-5">
                        <h1 class="display-3 text-white mb-3 animated slideInDown" style="font-weight: bold">STORE</h1>
                        <div class="mx-auto" style="width: 100%; max-width: 600px;">
